form.search-room(action="#")
  h1.search-room__header Найдём номера под ваши пожелания
  h2.search-room-header__arrival прибытие
  h2.search-room-header__departure  выезд
  
  input.search-room__arrival-date(type="text" placeholder="ДД.ММ.ГГГГ")
  .search-room__expand-more-arrival-date
    
  
  input.search-room__date-of-departure(type="text" placeholder="ДД.ММ.ГГГГ")
  .search-room__expand-more-date-of-departure
  
  
  h2.search-room-header__gast гости
  input.search-room__gast(type="text" placeholder="3 гостя")
  img.dropdown__expand-more(src="../img/expand_more.svg", alt="expand_more")

  
  .dropdown-gast-filled-in__form
    p.dropdown__p.first взрослые 
    p.dropdown__p дети
    p.dropdown__p младенцы
    .dropdown__circle-plus1
      p.dropdown-p__plus +
    .dropdown__circle-plus2
      p.dropdown-p__plus +
    .dropdown__circle-plus3 
      p.dropdown-p__plus +
    .adults 2
    .children 1
    .babies 0
    .dropdown__circle-minus1
      p.dropdown-p__minus -
    .dropdown__circle-minus2
      p.dropdown-p__minus -
    .dropdown__circle-minus3
      p.dropdown-p__minus.last -

    a.dropdown__link1(href="#") очистить
    a.dropdown__link2(href="#") применить
 
  .block-link-image
    a.search-room__pick-up-a-room(href="#") 
      span Подобрать номер
      svg.account-registration-link-image_pay(width='18' height='18' viewBox='0 0 18 18' fill='none' xmlns='http://www.w3.org/2000/svg')
        path(d='M9 0.984375L17.0156 9L9 17.0156L7.59375 15.6094L13.1719 9.98438H0.984375V8.01562H13.1719L7.59375 2.39062L9 0.984375Z')
        defs
          lineargradient#paint0_linear(x1='9' y1='-13' x2='9' y2='31' gradientUnits='userSpaceOnUse')
            stop(stop-color='#BC9CFF')
            stop(offset='1' stop-color='#8BA4F9')

  